<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/conf/tsmp" mode="merge"/>
    <filter root="/conf/global/settings/dam/adminui-extension/metadataschema/default" mode="replace"/>

    <filter root="/etc/reportData" mode="merge"/>

    <filter root="/content/tsmp" mode="merge"/>
    <filter root="/content/dam/tsmp" mode="merge">
        <exclude pattern="/content/dam/tsmp(/.*)?"/>
        <include pattern="/content/dam/tsmp/jcr:content(/.*)?"/>
    </filter>
    <filter root="/content/dam/tsmp/dropbox" mode="merge"/>
    <filter root="/content/dam/tsmp/asset.jpg" mode="merge"/>
    <filter root="/content/experience-fragments/tsmp" mode="merge"/>
    <filter root="/content/cq:tags" mode="merge"/>
</workspaceFilter>
